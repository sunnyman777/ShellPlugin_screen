<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    >
<RelativeLayout
    android:id="@+id/appfunc_search_result_item_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/gl_zero_screen_tab_item_selector">
 
    <!--  
 	 <LinearLayout
        android:id="@+id/search_result_list_item_icon_layout"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="9dp"
        android:background="@drawable/zero_screen_tab_deaful_icon"
        android:gravity="center" >

        <ImageView
            android:id="@+id/search_result_list_item_icon"
            android:layout_width="35dp"
            android:layout_height="35dp" />
    </LinearLayout>-->

 	
 	 <ImageView
            android:id="@+id/search_result_list_item_icon"
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="8.5dp"
            android:layout_marginBottom="8.5dp"
            android:layout_centerVertical="true"
            android:background="@drawable/gl_zero_screen_tab_default_icon_bg"
            android:src="@drawable/gl_zero_screen_tab_default_icon"/>
 	 
 	 <LinearLayout
    	android:id="@+id/search_result_list_item_sms_call_layout"
         android:layout_width="wrap_content"
         android:layout_height="match_parent"
         android:layout_alignParentRight="true"
         android:layout_centerVertical="true"
         android:gravity="center"
         android:orientation="horizontal" >
          
         <com.jiubang.shell.screen.zero.search.GLSearchLocalImageView
             android:id="@+id/search_result_list_item_call_icon"
             android:layout_height="wrap_content"
		     android:layout_width="wrap_content"
             android:background="@drawable/gl_zero_screen_tab_item_selector"
             android:src="@drawable/gl_zero_screen_tab_call"
             android:scaleType="centerInside" 
             android:focusable="false"/>

         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:background="@drawable/gl_zero_screen_tab_shu_line" />
        
          <com.jiubang.shell.screen.zero.search.GLSearchLocalImageView
            android:id="@+id/search_result_list_item_sms_icon"
            android:layout_height="wrap_content"
		    android:layout_width="wrap_content"
            android:background="@drawable/gl_zero_screen_tab_item_selector" 
            android:src="@drawable/gl_zero_screen_tab_sms"
            android:scaleType="centerInside"
            android:focusable="false"/>
            
     </LinearLayout>
        
 	 <LinearLayout 
 	     android:id="@+id/appfunc_search_result_list_item_info"
 	     android:layout_width="match_parent"
 	     android:layout_height="wrap_content"
 	     android:layout_toRightOf="@+id/search_result_list_item_icon"
		 android:layout_toLeftOf="@+id/search_result_list_item_sms_call_layout"
		 android:layout_marginLeft="9dp"
		 android:layout_centerVertical="true"
		 android:orientation="vertical"
 	     >
		 <com.jiubang.shell.common.component.ShellTextViewWrapper
	       android:id="@+id/appfunc_search_result_list_item_name"
	       android:layout_width="match_parent"
	       android:layout_height="wrap_content"
	       android:singleLine="true"
	       android:ellipsize="end"
	       android:textColor="#494949"
	       android:textSize="16dp"
	       android:text="123" />
		 <com.jiubang.shell.common.component.ShellTextViewWrapper
	       android:id="@+id/appfunc_search_result_list_item_phone"
	       android:layout_width="match_parent"
	       android:layout_height="wrap_content"
	       android:singleLine="true"
	       android:ellipsize="end"
	       android:textColor="#8c8c8c"
	       android:text="123"
	       android:textSize="14dp" />
 	 </LinearLayout>

    <ImageView
        android:id="@+id/appfunc_search_result_list_item_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:background="#c1c1c1"
        android:contentDescription="@string/app_name" />
    <!--
        android:background="@drawable/appfunc_search_result_list_divider"-->
 
</RelativeLayout>
</LinearLayout>