<?xml version="1.0" encoding="utf-8"?>
<com.jiubang.shell.appdrawer.search.GLAppDrawerSearchInputBar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="83dp"
    android:background="@drawable/gl_appdraw_search_top_search_bg"
    android:paddingLeft="16dp"
    android:paddingRight="16dp" >

    <EditText
        android:id="@+id/gl_search_edit_text"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="9.5dp"
        android:background="@drawable/gl_appdrawer_search_edit_text_bg"
        android:imeOptions="actionSearch|flagNoExtractUi"
        android:inputType="none"
        android:singleLine="true"
        android:textColor="#FEFEFE"
        android:textSize="16sp" />

    <ImageButton
        android:id="@+id/gl_search_clear_key_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="9.5dp"
        android:layout_alignRight="@+id/gl_search_edit_text"
        android:background="@drawable/gl_search_clear_key_btn_selector"
        android:contentDescription="search_clear_btn"
        android:visibility="invisible" />

</com.jiubang.shell.appdrawer.search.GLAppDrawerSearchInputBar><!--
