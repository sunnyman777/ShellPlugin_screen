<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/appfunc_search_result_item_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
 	android:paddingLeft="16dp"
    android:paddingRight="16dp"
    android:addStatesFromChildren="true"
    android:descendantFocusability="blocksDescendants"
    android:orientation="vertical">
 	
 	 <com.jiubang.shell.common.component.ShellTextViewWrapper
        android:id="@+id/appfunc_search_result_list_title"
        android:layout_width="match_parent"
        android:paddingLeft="9dp"
        android:layout_height="32dp"
 	    android:gravity="center_vertical"
        android:singleLine="true"
        android:textColor="#71a901"
        android:textSize="13sp"
        android:text="12333"
        />
 	 
 	 <ImageView
        android:id="@+id/appfunc_search_result_list_item_line"
        android:layout_width="match_parent"
        android:layout_height="1.5dp"
        android:background="#91c400"
        android:contentDescription="@string/app_name" />
 	 
</LinearLayout>